    fileName = lowerCode.replace(/ /g, '_').replace(/-/g, '_');
  }
  
  // æ£€æŸ¥æ˜¯å¦éœ€è¦ä½¿ç”¨PNGæ ¼å¼
  const ext = pngFlagList.includes(fileName) ? 'png' : 'svg';
  return `state_flags/${countryCode}/${fileName}.${ext}`;
}
